{
  "kind": "collectionType",
  "collectionName": "event_dates",
  "info": {
    "singularName": "event-date",
    "pluralName": "event-dates",
    "displayName": "Event",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "state": {
      "type": "enumeration",
      "enum": [
        "open",
        "expired",
        "deleted"
      ],
      "required": true,
      "default": "open"
    },
    "price": {
      "type": "decimal",
      "required": true,
      "default": 0
    },
    "schedules": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::schedule.schedule",
      "mappedBy": "event"
    },
    "attendee": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::attendee.attendee",
      "mappedBy": "event_date"
    }
  }
}
